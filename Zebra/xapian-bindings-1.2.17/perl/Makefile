install-perlarchLTLIBRARIES: $(perlarch_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	@list='$(perlarch_LTLIBRARIES)'; test -n "$(perlarchdir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(MKDIR_P) '$(DESTDIR)$(perlarchdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(perlarchdir)" || exit 1; \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(perlarchdir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(perlarchdir)"; \
	}

uninstall-perlarchLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(perlarch_LTLIBRARIES)'; test -n "$(perlarchdir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(perlarchdir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(perlarchdir)/$$f"; \
	done

clean-perlarchLTLIBRARIES:
	-test -z "$(perlarch_LTLIBRARIES)" || rm -f $(perlarch_LTLIBRARIES)
	@list='$(perlarch_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
